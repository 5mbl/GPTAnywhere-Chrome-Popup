{
    "name": "GPTAnywhere",
    "description": "Super Easy access to ChatGPT with a simple Chrome-Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "icons": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    },
    "action": {
        "default_popup": "frontend/index.html"
    },
    "host_permissions": ["https://*.openai.com/"],
    "background": {
      "service_worker": "background/index.js"
    }
  }